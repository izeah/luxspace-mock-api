{
  "version": 2,
  "functions": {
    "index.js": {
      "memory": 1024,
      "includeFiles": "Luxspace-Mock.postman_collection.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "index.js"
    }
  ],
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
